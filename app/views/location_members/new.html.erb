<form action="/members" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <h3>Enter new member:</h3>
  <input type='text' name='member[primary_member]'/>Member Name<br>
  <input type='checkbox' name='member[monthly_membership]'>Monthly Membership?</input><br>
  <input type='checkbox' name='member[yearly_membership]'></input>Yearly Membership?<br>
  <input type='number' name='member[people_in_membership]'>Number of people in membership</input><br>
  <input type='number' name='member[location_id]' value="<%= @member.location_id %>">Gym they belong to</input><br>
  <input type='submit'/>
</form>

<br>
<a href="/members">Members Index</a>
